<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Theme for the launch screen (before Flutter draws the first frame) -->
    <style name="LaunchTheme" parent="@style/Theme.MaterialComponents.Light.NoActionBar">
        <item name="android:windowBackground">@drawable/launch_background</item>
    </style>

    <!-- Theme applied after the app process has started -->
    <style name="NormalTheme" parent="@style/Theme.MaterialComponents.Light.NoActionBar">
        <item name="android:windowBackground">?android:colorBackground</item>
    </style>
</resources>
